IDR_MAINFRAME MENU 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "Open 3DS",                    ID_FILE_OPEN3DS
        MENUITEM "退出(&X)",                    ID_APP_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "撤消\tCtrl+Z(&U)",            ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "剪切\tCtrl+X(&T)",            ID_EDIT_CUT
        MENUITEM "复制\tCtrl+C(&C)",            ID_EDIT_COPY
        MENUITEM "粘贴\tCtrl+V(&P)",            ID_EDIT_PASTE
    END
    POPUP "视图(&V)"
    BEGIN
        MENUITEM "工具栏(&T)",                  ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                  ID_VIEW_STATUS_BAR
        MENUITEM "屏幕实时刷新",                ID_BUTTON_REFRESH
        MENUITEM "动画",                        ID_BUTTON_ANIM
        MENUITEM "仿射变换插值",                ID_BUTTON_AFFINE_TEX
        POPUP "几何背面剔除"
        BEGIN
            MENUITEM "CW顺时针",                    ID_VIEW_GEOM_CW
            MENUITEM "CCW逆时针",                   ID_VIEW_GEOM_CCW
            MENUITEM "无",                          ID_VIEW_GEOM_NOCULL
            MENUITEM "全部",                        ID_VIEW_GEOM_ALLCULL
        END
        POPUP "光栅器背面剔除"
        BEGIN
            MENUITEM "CW顺时针",                    ID_VIEW_RAST_CW
            MENUITEM "CCW逆时针",                   ID_VIEW_RAST_CCW
            MENUITEM "无",                          ID_VIEW_RAST_NOCULL
            MENUITEM "全部",                        ID_VIEW_RAST_ALLCULL
        END
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 softcore(&A)...",        ID_APP_ABOUT
    END
END


